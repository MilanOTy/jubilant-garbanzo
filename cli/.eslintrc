{
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": true,
    "jasmine": true,
    "node": true,
    "mocha": true,
    "browser": true,
    "builtin": true
  },
  "globals": {},
  "rules": {
    "arrow-parens": [2, "always"],
    "block-scoped-var": 2,
    "camelcase": 2,
    "comma-dangle": [2, {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "imports": "never",
      "exports": "never",
      "functions": "ignore"
    }],
    "curly": [2, "all"],
    "dot-notation": [2, {
      "allowKeywords": true
    }],
    "eqeqeq": [2, "allow-null"],
    "new-cap": [2, {
      "capIsNewExceptions": ["express.Router"]
    }],
    "no-bitwise": 2,
    "no-caller": 2,
    "no-cond-assign": [2, "except-parens"],
    "no-console":2,
    "no-debugger": 2,
    "no-empty": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-mixed-spaces-and-tabs": "error",
    "no-multi-str": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-var": 2,
    "no-with": 2,
    "prefer-const": [2, {
      "destructuring": "any"
    }],
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "strict": 2,
    "valid-typeof": 2,
    "wrap-iife": [2, "inside"]
  }
}
